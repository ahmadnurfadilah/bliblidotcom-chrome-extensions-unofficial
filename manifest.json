{
  "manifest_version": 2,
  "name": "bliblidotcom-chrome-ext",
  "description": "Blibli.com Chrome Extensions Unofficial Version",
  "version": "1.0",
  "browser_action":
  {
      "default_icon": "icon.png",
      "default_popup": "blibli.html",
      "default_title": "Toko Online Blibli.com, Sensasi Belanja Online Shop ala Mall"
  },
  "content_scripts": [
  {
      "matches": [
          "<all_urls>"
      ],
      "js": ["app.js"]
  }],
  "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/"
  ]
}